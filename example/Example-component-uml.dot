@startuml
skinparam componentStyle uml2

skinparam

interface  {
	backgroundColor RosyBrown
	borderColor orange
}

sprite $dependency [16x16/16] {
	FFFFFFFFFFFFFFFF
	FFFFFFFFFFFFFFFF
	FFFFFFFFFFFFFFFF
	FFFFFFFFFFFFFFFF
	FFFFFFFFFF0FFFFF
	FFFFFFFFFF00FFFF
	FF00000000000FFF
	FF000000000000FF
	FF00000000000FFF
	FFFFFFFFFF00FFFF
	FFFFFFFFFF0FFFFF
	FFFFFFFFFFFFFFFF
	FFFFFFFFFFFFFFFF
	FFFFFFFFFFFFFFFF
	FFFFFFFFFFFFFFFF
	FFFFFFFFFFFFFFFF
}
 
node "service: user" {
	component "user" as user
	interface "UserSoap 6081" as UserSoap #Red
	[] -[#Red]-> () UserSoap : exposes
	interface "AuthSoap 6082" as AuthSoap #Lightgrey
	[] -[#Lightgrey]-> () AuthSoap : exposes
	interface "UserGrpc 7070" as UserGrpc #Red
	[] -[#Red]-> () UserGrpc : exposes
	interface "UserRest 8081" as UserRest #Red
	[] -[#Red]-> () UserRest : exposes
	interface "UserSoap 6081" as UserSoap #Blue
	[user] -[#Blue]-> () UserSoap : exposes
	interface "AuthSoap 6082" as AuthSoap #Lightgrey
	[user] -[#Lightgrey]-> () AuthSoap : exposes
	interface "UserGrpc 7070" as UserGrpc #Blue
	[user] -[#Blue]-> () UserGrpc : exposes
	interface "UserRest 8081" as UserRest #Blue
	[user] -[#Blue]-> () UserRest : exposes
}
 
 
node "service: product" {
	component "product" as product
	interface "ProductGrpc 7071" as ProductGrpc #Red
	[] -[#Red]-> () ProductGrpc : exposes
	interface "ProductRest 8081" as ProductRest #Red
	[] -[#Red]-> () ProductRest : exposes
	interface "ProductGrpc 7071" as ProductGrpc #Blue
	[product] -[#Blue]-> () ProductGrpc : exposes
	interface "ProductRest 8081" as ProductRest #Blue
	[product] -[#Blue]-> () ProductRest : exposes
}
 
[product] -[#Blue]-> ()  : consumes
 
