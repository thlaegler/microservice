<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="architectureYaml" nsURI="http://www.laegler.com/microservice/modeler/ArchitectureYaml"
    nsPrefix="architectureYaml">
  <eClassifiers xsi:type="ecore:EClass" name="Architecture">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="packageName" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="yamls" upperBound="-1"
        eType="#//Yaml" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Yaml">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="yamlFile" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="yamlRoot" eType="#//YamlNode"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="yamlInline" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="YamlFile">
    <eStructuralFeatures xsi:type="ecore:EReference" name="yamlFile" eType="#//WorkspaceFile"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="YamlRoot">
    <eStructuralFeatures xsi:type="ecore:EReference" name="yamlRoot" eType="#//YamlNode"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="YamlInline">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="yamlInline" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="YamlType">
    <eLiterals name="FILE" literal="file"/>
    <eLiterals name="NODE" value="1" literal="node"/>
    <eLiterals name="INLINE" value="2" literal="inline"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="WorkspaceFile">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="location" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="YamlNode">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="nodes" upperBound="-1"
        eType="#//YamlNode" containment="true"/>
  </eClassifiers>
</ecore:EPackage>
